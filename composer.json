{
    "name" : "meethour/meethour-ical-generator",
    "description" : "Build calendars in the iCalendar format",
    "keywords" : [
        "spatie",
        "calendar",
        "ics",
        "ical",
        "icalendar"
    ],
    "homepage" : "https://github.com/v-empower/meethour-ical-generator",
    "license" : "MIT",
    "authors" : [
        {
            "name" : "Meet Hour LLC",
            "email" : "hello@meethour.io",
            "homepage" : "https://meethour.io"
        }
    ],

    "require": {
        "php": "^7.4|^8.0|^8.2",
        "ext-mbstring": "*",
        "nesbot/carbon": "^2.41",
        "spatie/enum": "^3.0"
    },
    "require-dev" : {
        "larapack/dd" : "^1.0",
        "phpunit/phpunit" : "^8.2",
        "spatie/phpunit-snapshot-assertions" : "^4.2",
        "vimeo/psalm" : "^4.3",
        "ext-json" : "*"
    },
    "autoload" : {
        "psr-4" : {
            "Spatie\\IcalendarGenerator\\" : "icalendar-generator/src"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Spatie\\IcalendarGenerator\\Tests\\" : "enum/tests"
			
        }
    },
    "scripts" : {
        "test" : "vendor/bin/phpunit",
        "psalm" : "vendor/bin/psalm",
        "test-coverage" : "vendor/bin/phpunit --coverage-html coverage"
    },
    "config" : {
        "sort-packages" : true
    }
}
